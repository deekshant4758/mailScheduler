{
  "name": "reachinbox-scheduler-backend",
  "version": "1.0.0",
  "description": "Production-grade email scheduler with BullMQ and Redis",
  "main": "dist/server.js",
  "scripts": {
    "migrate": "node runMigration.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "node -r ts-node/register migrations/run.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "email",
    "scheduler",
    "bullmq",
    "redis"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/nodemailer": "^7.0.5",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/node": "^25.0.9",
    "bullmq": "^5.66.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "knex": "^3.1.0",
    "mysql2": "^3.16.1",
    "nodemailer": "^7.0.12",
    "uuid": "^13.0.0"
  }
}
